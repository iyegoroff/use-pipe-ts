{
  "name": "use-pipe-ts",
  "version": "1.0.4",
  "description": "Pipe-based useCallback react hook",
  "sideEffects": false,
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "react-native": "./dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "type": "module",
  "scripts": {
    "preversion": "npm run build",
    "postversion": "git add . && git commit -am lint || : && git push && git push --tags",
    "build": "rm -rf dist && rollup -c rollup.config.ts --configPlugin typescript && rm -rf tmp"
  },
  "keywords": [
    "react-hook",
    "typescript"
  ],
  "files": [
    "dist",
    "src"
  ],
  "author": "iyegoroff <iegoroff@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iyegoroff/use-pipe-ts/issues"
  },
  "homepage": "https://github.com/iyegoroff/use-pipe-ts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iyegoroff/use-pipe-ts.git"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/react": "^17.0.33",
    "pipe-ts": "^0.0.9",
    "rollup": "^2.73.0",
    "rollup-plugin-dts": "^4.2.1",
    "typescript": "^4.6.4"
  },
  "peerDependencies": {
    "pipe-ts": "*",
    "react": "*"
  }
}
